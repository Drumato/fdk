<network>
  <name>fdk</name>
  <uuid>11111111-1111-1111-1111-111111111111</uuid>
  <forward mode='nat'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='fdk0' stp='on' delay='0'/>
  <mac address='52:54:00:aa:aa:aa'/>
  <ip address='192.168.133.1' netmask='255.255.255.0'>
    <dhcp>
      <range start='192.168.133.100' end='192.168.133.199'/>
      {% for item in groups['machines'] %}
      <host mac='{{ hostvars[item].mac }}' ip='{{ hostvars[item].ansible_host }}'/>
      {% endfor %}
    </dhcp>
  </ip>
</network>
